alias ls="ls --color=auto"
alias ll="ls -alh"
alias ip="ip -c"
alias grep="grep --color=auto"
alias apt4="apt -o Acquire::ForceIPv4=true"
alias apt6="apt -o Acquire::ForceIPv6=true"
alias gsudo='sudo git -c "include.path=/home/mikachu/.config/git/config" -c "include.path=/home/mikachu/.gitconfig"'
alias g="git"
alias env="source env/bin/activate"
